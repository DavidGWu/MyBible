<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="fill_parent"
              android:layout_height="fill_parent" android:paddingLeft="20dip" android:paddingRight="20dip"
              android:paddingBottom="20dip" android:background="#ff71e9ff" android:id="@+id/strongConcordanceLayout"
              >

    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content">
        <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/dismiss"
                android:id="@+id/Button_strong_dismiss"/>
        <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/strong_search"
                android:id="@+id/button_strong_search"/>
    </LinearLayout>
    <WebView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:id="@+id/strongConcordanceWebView" android:layout_gravity="left|center_horizontal"
            android:padding="10dp"/>
</LinearLayout>